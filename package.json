{
  "name": "vbao-product-size",
  "version": "",
  "description": "",
  "author": "",
  "license": "",
  "engines": {
    "node": ">=6.13.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "clearMocks": true,
    "preset": "@shelf/jest-mongodb",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "**/server/*.{js,jsx}",
      "**/server/database/*.{js,jsx}",
      "**/client/ProductSize*.jsx",
      "!**/node_modules/**",
      "!**/server/server.js",
      "!**/server/database/seed.js"
    ],
    "globals": {
      "window": {}
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "scripts": {
    "generate-data": "node ./server/database/seed.js",
    "seed-mongo": "mongoimport --db ikea --collection productsizes --type json --file ./server/database/__data__/product-size.json --jsonArray",
    "test": "jest --no-cache -u --collect-coverage",
    "start": "nodemon ./server/server.js",
    "build": "webpack --watch",
    "start-docker": "node ./server/server.js",
    "postgres-tables": "nodemon ./server/database/postgres/postgresModels.js",
    "write-csv": "node ./server/database/writeCSV.js"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@shelf/jest-mongodb": "^1.2.3",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.3",
    "enzyme": "^3.11.0",
    "eslint-config-hackreactor": "github:reactorcore/eslint-config-hackreactor",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "style-loader": "^2.0.0",
    "supertest": "^6.1.3",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "csv-write-stream": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.19",
    "newrelic": "^7.5.0",
    "pg": "^8.6.0",
    "pg-cursor": "^2.6.0",
    "postgres": "^1.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
